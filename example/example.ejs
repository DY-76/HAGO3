               console.log(reader);
               reader.readAsText(file, "utf-8");
               reader.onload=function(){
                   var view=document.getElementById("content");
                   view.textContent = reader.result;
                   
                   var load_data = JSON.parse(reader.result);
                   Entry.clearProject();
                   
                  Entry.loadProject(load_data);

               };
               var load_data = JSON.parse(reader.result);
